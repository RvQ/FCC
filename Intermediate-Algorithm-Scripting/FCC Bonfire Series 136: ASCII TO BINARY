function asciiAgent(asciiString){
	var result = [];
		asciiArray = asciiString.split('');

	asciiArray.forEach(function(asciiCode){
		var decimal = asciiCode.charCodeAt();
		var binary = decimal.toString(2)
		result.push(binary)
	});
	return result.join(' ')
}



asciiAgent("Mida paganast siin k√ºll toimub ma ei saa aru")
